{
    "name": "php-di/silex-bridge",
    "description": "PHP-DI integration in Silex",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "DI\\Bridge\\Silex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Bridge\\Silex\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "php-di/php-di": "~5.0",
        "php-di/invoker": "~1.3",
        "silex/silex" : "~2.0",
        "pimple/pimple" : "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36 | ^5.7.21",
        "twig/twig": "~1.8",
        "swiftmailer/swiftmailer": "^5.4"
    }
}
